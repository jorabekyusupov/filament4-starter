{
  "name": "jora/fil-theme",
  "description": "A modern, responsive, and customizable theme for FilamentPHP, designed to elevate your admin panel with a sleek interface and seamless user experience.",
  "keywords": [
    "filament",
    "filamentphp",
    "laravel",
    "theme",
    "admin panel",
    "dashboard"
  ],
  "version": "1.0.0",
  "homepage": "https://github.com/jorabekyusupov",
  "support": {
    "issues": "https://github.com/jorabekyusupov",
    "source": "https://github.com/jorabekyusupov"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jorabek",
      "email": "x@jora.uz",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "filament/filament": "^4.0",
    "spatie/laravel-package-tools": "^1.15.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Jora\\FilTheme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Resma\\FilamentAwinTheme\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
    "lint": "pint",
    "test": "pest",
    "test:lint": "pint --test"
  },
  "extra": {
    "laravel": {
      "providers": [
      ],
      "aliases": {
        "FilamentAwinTheme": "Jora\\FilTheme\\Facades\\FilTheme"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}